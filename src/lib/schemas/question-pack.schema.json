{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": ["packId", "subject", "topic", "difficulty", "questions"],
  "properties": {
    "packId": { "type": "string" },
    "subject": { "type": "string" },
    "topic": { "type": "string" },
    "difficulty": { "type": "string" },
    "questions": {
      "type": "array",
      "minItems": 1,
      "items": { "$ref": "question" }
    }
  }
}
